<%- include('partials/header.ejs') %>
<h1>Home</h1>
<p>
  <%= homeStartingContent %>
</p>
<% posts.forEach(function(post) { %>
  <h2><%= post.title %> </h2>
  <p>
    <%= post.post.substring(0, 100) %>
    <a href="/posts/<%= post._id %>">Read more</a>
  </p>
<% }) %>
<%- include('partials/footer.ejs') %>
